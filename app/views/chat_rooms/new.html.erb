<div class='container-fluid'>
  <h1>Add chat room</h1>
  <div class='row'>
    <div class='col-md-4' >
      <%= form_for @chat_room do |f| %>
        <div class="form-group">
          <%= f.label :title %>
          <%= f.text_field :title, autofocus: true, class: 'form-control' %>
        </div>

        <%= f.submit "Add!", class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>