<!DOCTYPE html>
<html>
  <head>
    <title>Wordapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application',  media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'confirmation', media: 'all' %>
    <%= stylesheet_link_tag    'session',      media: 'all' %>
    <%= stylesheet_link_tag    'navbar',       media: 'all' %>
    <%= stylesheet_link_tag    'chat_rooms',   media: 'all' %>
    <%= stylesheet_link_tag    'messages',     media: 'all' %>
    <%= stylesheet_link_tag    'users',        media: 'all' %>
    <%= stylesheet_link_tag    'registrations',        media: 'all' %>
    <script src="//maps.google.com/maps/api/js?key=AIzaSyBwl5chO-5r9FaxNOIfltMfAnXhwwdTPH0"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class='ren-nav'>
      <%= render 'layouts/navbar' %>
    </div>
    <%= render 'layouts/alert'%>
    <%= render 'layouts/footer'%>
      <%= yield %>
  </body>
</html>
